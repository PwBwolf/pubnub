var express = require('express');
var router = express.Router();


router.post('/create/channel/channelGroup', function (req, res, next) {
    res.send('create mongo channel doc');
    res.send('update users metadata');
    res.send('alerting pubnub of new channel and subscribers');
});

router.put('/displayName', function (req, res, next) {
    res.send('update channel metadata with new display name');
});

module.exports = router;